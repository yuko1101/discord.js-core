{
  "name": "discord.js-core",
  "version": "2.1.0",
  "description": "Multi-functional library for discord.js bot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "lint": "npx eslint . --config .eslintrc.json --ext .js,.jsx,.cjs,.mjs,.ts,.tsx --ignore-path .eslintignore",
    "lint:fix": "npx eslint . --config .eslintrc.json --ext .js,.jsx,.cjs,.mjs,.ts,.tsx --ignore-path .eslintignore --fix",
    "build": "node scripts/clean.js && npx tsc",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuko1101/discord.js-core.git"
  },
  "keywords": [
    "discord.js",
    "discord",
    "bot",
    "core",
    "library"
  ],
  "author": "yuko1101 (https://github.com/yuko1101)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuko1101/discord.js-core/issues"
  },
  "homepage": "https://github.com/yuko1101/discord.js-core#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "config_file.js": "^1.2.4",
    "discord.js": "^14.13.0"
  }
}
